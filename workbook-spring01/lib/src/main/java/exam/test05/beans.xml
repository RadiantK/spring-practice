<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="score1" class="exam.test05.Score">
		<property name="name"><value>일자바</value></property>
		<property name="kor"><value>80</value></property>
		<property name="eng"><value>80</value></property>
		<property name="math"><value>80</value></property>
	</bean>
	
	<bean id="score2" class="exam.test05.Score">
		<property name="name" value="이순신" />
		<property name="kor" value="90" />
		<property name="eng" value="90" />
		<property name="math" value="90" />
	</bean>
</beans>